    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.7",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
