{
  "name": "@rollenasistores/devsum",
  "version": "1.7.7",
  "description": "DevSum CLI automates your Git workflow with AI-powered commit messages, smart branch management, and generates polished accomplishment summaries. Perfect for efficient development workflows, performance reviews, and sprint reports.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "devsum": "./dist/bin/devsum.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run build:copy-assets",
    "build:copy-assets": "cp -r bin dist/ && chmod +x dist/bin/devsum.js",
    "dev": "tsx bin/devsum.ts",
    "start": "node dist/bin/devsum.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'Linting disabled - using TypeScript compiler for validation'",
    "lint:fix": "echo 'Linting disabled - using TypeScript compiler for validation'",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "prepublishOnly": "npm run validate && npm run build",
    "postversion": "git push && git push --tags",
    "release": "standard-version",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "precommit": "npm run validate",
    "ci": "npm run validate && npm run test:coverage"
  },
  "keywords": [
    "cli",
    "git",
    "ai",
    "reports",
    "typescript",
    "accomplishment",
    "performance-review",
    "commit-analysis",
    "developer-tools",
    "gemini",
    "claude",
    "anthropic",
    "google-ai",
    "productivity",
    "automation",
    "markdown",
    "sprint-report",
    "project-management",
    "code-review",
    "development-summary",
    "commit-automation",
    "branch-management",
    "git-workflow",
    "conventional-commits",
    "workflow-automation"
  ],
  "author": {
    "name": "Rollen Asistores",
    "email": "asistoresrlc1@gmail.com",
    "url": "https://github.com/rollenasistores"
  },
  "contributors": [
    {
      "name": "Rollen Asistores",
      "email": "asistoresrlc1@gmail.com",
      "url": "https://github.com/rollenasistores"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollenasistores/devsum.git"
  },
  "bugs": {
    "url": "https://github.com/rollenasistores/devsum/issues"
  },
  "homepage": "http://devsum.rollenasistores.site/",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rollenasistores"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.15",
    "marked": "^16.2.1",
    "open": "^10.1.0",
    "openai": "^5.20.2",
    "puppeteer": "^24.19.0",
    "simple-git": "^3.25.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
