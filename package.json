    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollenasistores/devsum.git"
  },
  "bugs": {
    "url": "https://github.com/rollenasistores/devsum/issues"
  },
  "homepage": "https://github.com/rollenasistores/devsum#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rollenasistores"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.15",
    "marked": "^16.2.1",
    "openai": "^5.20.2",
    "puppeteer": "^24.19.0",
    "simple-git": "^3.25.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "preferGlobal": true,
  "os": [
