{
  "name": "@rollenasistores/devsum-dev",
  "version": "1.5.4",
  "description": "Development tools and performance testing for DevSum CLI",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/check-requirements.js",
    "postinstall": "node scripts/setup.js",
    "build": "npm run clean && tsc && npm run build:copy-assets",
    "build:copy-assets": "cp -r bin dist/ && chmod +x dist/bin/devsum.js",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer dist/",
    "dev": "tsx bin/devsum.ts",
    "start": "node dist/bin/devsum.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'Linting disabled - using TypeScript compiler for validation'",
    "lint:fix": "echo 'Linting disabled - using TypeScript compiler for validation'",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "prepublishOnly": "npm run validate && npm run build",
    "postversion": "git push && git push --tags",
    "release": "standard-version",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "precommit": "npm run validate",
    "ci": "npm run validate && npm run test:coverage",
    "install:full": "npm install --save-optional @anthropic-ai/sdk @google/generative-ai openai puppeteer inquirer",
    "install:core": "npm install --no-optional",
    "optimize": "node scripts/optimize-bundle.js",
    "analyze": "npm run build && npm run optimize",
    "benchmark": "node scripts/benchmark-installation.js",
    "benchmark:all": "npm run benchmark",
    "monitor": "node scripts/monitor-performance.js",
    "monitor:all": "npm run monitor all",
    "compare": "node scripts/compare-performance.js",
    "check:regression": "node scripts/check-regression.js",
    "perf:test": "npm run benchmark && npm run monitor && npm run compare",
    "perf:ci": "npm run build && npm run perf:test",
    "dev-mode": "node scripts/dev-mode.js",
    "commit": "tsx scripts/commit.ts"
  },
  "keywords": [
    "cli",
    "git",
    "ai",
    "reports",
    "development",
    "performance",
    "testing",
    "benchmark"
  ],
  "author": "Rollen Asistores <asistoresrlc1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollenasistores/devsum.git"
  },
  "bugs": {
    "url": "https://github.com/rollenasistores/devsum/issues"
  },
  "homepage": "https://github.com/rollenasistores/devsum#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rollenasistores"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "marked": "16.2.1",
    "simple-git": "3.25.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@google/generative-ai": "^0.24.1",
    "inquirer": "^9.2.15",
    "openai": "^5.20.2",
    "puppeteer": "^24.19.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "np": "^10.0.5",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
