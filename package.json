{
  "name": "@rollenasistores/devsum",
  "version": "1.3.0",
  "description": "ðŸš€ AI-powered CLI tool that generates professional accomplishment reports from git commits. Transform your development work into compelling narratives for performance reviews, sprint summaries, and project updates.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "devsum": "./dist/bin/devsum.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx bin/devsum.ts",
    "start": "node dist/bin/devsum.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run typecheck && npm run build",
    "postversion": "git push && git push --tags",
    "release": "standard-version",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "keywords": [
    "cli",
    "git",
    "ai",
    "reports",
    "typescript",
    "accomplishment",
    "performance-review",
    "commit-analysis",
    "developer-tools",
    "gemini",
    "claude",
    "anthropic",
    "google-ai",
    "productivity",
    "automation",
    "markdown",
    "sprint-report",
    "project-management",
    "code-review",
    "development-summary"
  ],
  "author": {
    "name": "Rollen Asistores",
    "email": "asistoresrlc1@gmail.com",
    "url": "https://github.com/rollenasistores"
  },
  "contributors": [
    {
      "name": "Rollen Asistores",
